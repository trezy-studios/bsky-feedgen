generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider      = "mysql"
  url           = env("DATABASE_URL")
  relationMode  = "prisma"
}

model Feed {
  description String
  name        String
  ownerDID    String
  rkey        String  @id @unique
  skeets      Skeet[]
}

model Skeet {
  cid         String
  feeds       Feed[]
  indexedAt   DateTime?     @default(now())
  replyParent String?
  replyRoot   String?
  uri         String        @id @unique
}
