generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider      = "mysql"
  url           = env("DATABASE_URL")
  relationMode  = "prisma"
}

enum FeedRecord {
  GAME_DEV
  GAME_NEWS
}

model Skeet {
  cid         String
  feedRecord  FeedRecord  @default(GAME_DEV)
  indexedAt   DateTime?   @default(now())
  replyParent String?
  replyRoot   String?
  uri         String      @id @unique
}
